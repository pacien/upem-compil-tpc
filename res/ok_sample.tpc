const special = 'b';

caractere funky_func(entier arg) {
  return !arg;
}

void main(void) {
  caractere ret;
  ret = 1 + funky_func(0) * 2;

  while (ret) {
    readc(ret);
    print(ret);

    if (ret == special) print('!');
  }
}

